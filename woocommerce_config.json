{
  "woocommerce": {
    "base_url": "https://store.themerrittmethod.com/wp-json/wc/v3",
    "consumer_key": "ck_your_consumer_key_here",
    "consumer_secret": "cs_your_consumer_secret_here"
  },
  "sync_settings": {
    "batch_size": 10,
    "rate_limit_delay": 2,
    "timeout": 30,
    "auto_update_existing": true,
    "backup_before_sync": true
  },
  "product_defaults": {
    "type": "simple",
    "status": "publish",
    "catalog_visibility": "visible",
    "manage_stock": false,
    "stock_status": "instock"
  }
}