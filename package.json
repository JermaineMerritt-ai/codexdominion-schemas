    "supertest": "^6.3.3",
    "tough-cookie": "^6.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "codexDominion": {
    "sovereignty_level": "maximum",
    "flame_status": "eternal",
    "empire_version": "2.0.0",
    "digital_independence": true,
    "council_approved": true,
    "sacred_seal": "authenticated",
    "last_updated": "2025-11-09",
    "status": "operational",
    "components": {
      "frontend": "active",
      "backend": "active",
      "dashboard": "active",
      "council": "active",
      "flame": "eternal",
      "sovereignty": "supreme"
    }
  },
  "config": {
    "default_port": 3000,
    "dashboard_port": 8050,
    "api_port": 8000,
    "websocket_port": 8080,
