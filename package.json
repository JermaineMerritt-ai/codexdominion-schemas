    "inquirer": "^9.2.11",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "ora": "^8.0.1",
    "socket.io": "^4.7.4",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.52.0",
    "form-data": "^4.0.4",
    "jest": "^25.0.0",
    "js-yaml": "^4.1.1",
    "jsdom": "^27.2.0",
    "node-notifier": "^10.0.1",
