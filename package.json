{
  "name": "codex-dominion",
  "version": "2.0.0",
  "description": "Complete Digital Empire - Codex Dominion Sovereignty Suite",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "python -m build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "python -m pytest",
    "deploy": "npm run build && docker-compose up -d",
    "dashboard": "streamlit run codex-suite/apps/dashboard/codex_unified.py --server.port 8050",
    "council": "streamlit run codex-suite/apps/dashboard/council_access.py --server.port 8051",
    "flame": "python cosmic_status_terminal.py",
    "sovereignty": "python final_verification.py",
    "proxy": "node aistorelab-proxy.js",
    "proxy-dev": "nodemon aistorelab-proxy.js",
    "install-all": "npm install && cd frontend && npm install",
    "clean": "rm -rf node_modules package-lock.json && cd frontend && rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean && npm run install-all",
    "status": "node empire-status.js",
    "validate": "node validate-empire.js",
    "auto-fix": "node auto-fix-empire.js",
    "fix-all": "npm run auto-fix",
    "diagnose": "node deep-diagnostics.js",
    "doctor": "npm run diagnose",
    "fix": "npm audit fix && cd frontend && npm audit fix"
  },
  "keywords": [
    "digital-sovereignty",
    "ai-development",
    "automation",
    "dashboard",
    "codex",
    "dominion",
    "empire",
    "streamlit",
    "python",
    "javascript"
  ],
  "author": "Codex Dominion High Council",
  "license": "CODEX-SOVEREIGN-LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codex-dominion/digital-empire.git"
  },
  "bugs": {
    "url": "https://github.com/codex-dominion/digital-empire/issues"
  },
  "homepage": "https://codex-dominion.dev",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0",
    "python": ">=3.9.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.21.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "framer-motion": "^12.23.24",
